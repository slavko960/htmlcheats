function TR(icon,lbl,tag,k){return '<div class="TR" data-k="'+k+'"><div class="SQ" id="sq-'+k+'"></div>'+I(icon)+'<span class="TL">'+lbl+'</span><span class="TG">'+tag+'</span></div>';}
function SL(icon,lbl,id,mn,mx,vl,st){return '<div class="SLR"><div class="ST"><div class="SL">'+I(icon)+'<span class="SB">'+lbl+'</span></div><span class="SV" id="sv-'+id+'">'+vl+'</span></div><div class="TRK" id="st-'+id+'" data-mn="'+mn+'" data-mx="'+mx+'" data-vl="'+vl+'" data-st="'+st+'"><div class="TF" id="sf-'+id+'"></div></div></div>';}
function IR(icon,lbl,id,val){return '<div class="IR"><div class="IR-I">'+I(icon)+'</div><div class="IR-L">'+lbl+'</div><input class="IR-N" id="I-'+id+'" value="'+val+'" autocomplete="off"><button class="IR-B" id="B-'+id+'">SET</button></div>';}
function INF(icon,lbl,id){return '<div class="INF"><div class="IL">'+I(icon)+''+lbl+'</div><div class="IV" id="'+id+'">—</div></div>';}
function SH(icon,lbl){return '<div class="SH">'+I(icon)+lbl+'</div>';}
function SEC(c){return '<div class="SEC">'+c+'</div>';}
function BR(c){return '<div class="BR">'+c+'</div>';}
function BT(icon,lbl,id,cls){return '<button class="BT'+(cls?' '+cls:'')+'" id="B-'+id+'">'+I(icon)+lbl+'</button>';}
function COL(c){return '<div class="COL">'+c+'</div>';}
function PNL(id,c,on){return '<div class="PNL'+(on?' on':'')+'" id="P-'+id+'">'+c+'</div>';}
function TAB(icon,lbl,id,on){return '<div class="TAB'+(on?' on':'')+'" data-t="'+id+'">'+I(icon)+lbl+'</div>';}

