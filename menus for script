var rt=window.__RT||(typeof cr_getC2Runtime==='function'?cr_getC2Runtime():null);
function getP(){var t=rt&&rt.types['t51'];return t&&t.instances[0]?t.instances[0]:null;}
function getE(){var t=rt&&rt.types['t50'];return t?t.instances:[];}
function getG(){var t=rt&&rt.types['t60'];return t&&t.instances[0]?t.instances[0]:null;}
function fT(l){var t=rt&&rt.types['t114'];if(!t)return null;for(var i=0;i<t.instances.length;i++)if(t.instances[i].instance_vars[0]===l)return t.instances[i];return null;}

var IV={};
function clr(k){if(IV[k]){clearInterval(IV[k]);delete IV[k];}}

var C={
  godMode:function(on){
    if(rt)rt.all_global_vars[3].data=on?1:0;
    clr('god');
    if(on)IV['god']=setInterval(function(){var p=getP();if(!p)return;var v=p.instance_vars;v[20]=v[18];v[21]=v[18];v[73]=v[18];v[23]=v[22];v[74]=v[22];v[38]=0;v[8]=0;},50);
  },
  instantKill:function(on){
    clr('ik');
    if(on)IV['ik']=setInterval(function(){getE().forEach(function(e){e.instance_vars[20]=-9999;e.instance_vars[21]=-9999;e.instance_vars[73]=-9999;});},16);
  },
  freeze:function(on){
    clr('frz');
    if(on){
      IV['frz']=setInterval(function(){
        getE().forEach(function(e){
          e.instance_vars[17]=0;e.instance_vars[37]=0;e.instance_vars[41]=0;e.instance_vars[24]=0;
          if(e.behavior_insts)e.behavior_insts.forEach(function(b){if('maxSpeed'in b)b.maxSpeed=0;if('speed'in b)b.speed=0;if('vel'in b)b.vel=0;if('maxvel'in b)b.maxvel=0;if('acc'in b)b.acc=0;});
        });
      },16);
    }else{
      getE().forEach(function(e){e.instance_vars[17]=265;e.instance_vars[37]=265;e.instance_vars[41]=4.7;e.instance_vars[24]=50;});
    }
  },
  setSpd:function(v){clr('spd');IV['spd']=setInterval(function(){var p=getP();if(!p)return;p.instance_vars[17]=v;p.instance_vars[37]=v;},16);},
  stopSpd:function(){clr('spd');var p=getP();if(p){p.instance_vars[17]=265;p.instance_vars[37]=265;}},
  setAtk:function(v){clr('atk');IV['atk']=setInterval(function(){var p=getP();if(!p)return;p.instance_vars[33]=v;},50);},
  stopAtk:function(){clr('atk');},
  setHP:function(v){var p=getP();if(!p)return;p.instance_vars[18]=v;p.instance_vars[20]=v;p.instance_vars[21]=v;p.instance_vars[73]=v;},
  setGold:function(v){var g=getG();if(g)g.instance_vars[0]=v;var p=getP();if(p)p.instance_vars[71]=v;var t=fT('Gold');if(t)t.instance_vars[1]=String(v);},
  setLevel:function(v){var p=getP();if(p)p.instance_vars[5]=v;},
  setXP:function(v){var p=getP();if(p)p.instance_vars[45]=v;},
  setKills:function(v){var p=getP();if(p)p.instance_vars[35]=v;},
  killAll:function(){getE().forEach(function(e){e.instance_vars[20]=-9999;e.instance_vars[21]=-9999;e.instance_vars[73]=-9999;});},
  stopAll:function(){
    Object.keys(IV).forEach(function(k){clearInterval(IV[k]);});IV={};
    if(rt)rt.all_global_vars[3].data=0;
    var p=getP();if(p){p.instance_vars[17]=265;p.instance_vars[37]=265;}
    getE().forEach(function(e){e.instance_vars[17]=265;e.instance_vars[37]=265;e.instance_vars[41]=4.7;e.instance_vars[24]=50;});
  }
};

var ST={godMode:false,instantKill:false,freeze:false,spdLock:false,atkLock:false,watchTitle:false};
var STV={spd:265,atk:0.20,hp:1200};

function msg(t){var e=document.getElementById('__EST');if(e)e.textContent=t;}

var TM={
  godMode:function(on){C.godMode(on);msg('God Mode '+(on?'ON':'OFF'));},
  instantKill:function(on){C.instantKill(on);msg('Instant Kill '+(on?'ON':'OFF'));},
  freeze:function(on){C.freeze(on);msg('Freeze '+(on?'ON':'OFF'));},
  spdLock:function(on){if(on)C.setSpd(STV.spd);else C.stopSpd();msg('Speed Lock '+(on?'ON':'OFF'));},
  atkLock:function(on){if(on)C.setAtk(STV.atk);else C.stopAtk();msg('Atk Lock '+(on?'ON':'OFF'));},
  watchTitle:function(on){if(!on)document.title='WarCall.io';msg('Watch '+(on?'ON':'OFF'));}
};

function setTog(k,on){
  ST[k]=on;
  var sq=document.getElementById('sq-'+k);
  var row=sq?sq.closest('.TR'):null;
  if(sq)on?sq.classList.add('on'):sq.classList.remove('on');
  if(row)on?row.classList.add('on'):row.classList.remove('on');
  if(TM[k])TM[k](on);
}

menu.querySelectorAll('.TAB').forEach(function(t){
  t.addEventListener('click',function(){
    menu.querySelectorAll('.TAB').forEach(function(x){x.classList.remove('on');});
    menu.querySelectorAll('.PNL').forEach(function(x){x.classList.remove('on');});
    t.classList.add('on');
    var p=document.getElementById('P-'+t.dataset.t);if(p)p.classList.add('on');
  });
});

menu.querySelectorAll('.TR[data-k]').forEach(function(r){
  r.addEventListener('click',function(){setTog(r.dataset.k,!ST[r.dataset.k]);});
});

function initSlider(id,cb){
  var tr=document.getElementById('st-'+id);if(!tr)return;
  var mn=parseFloat(tr.dataset.mn),mx=parseFloat(tr.dataset.mx),st=parseFloat(tr.dataset.st),vl=parseFloat(tr.dataset.vl);
  var fill=document.getElementById('sf-'+id),valEl=document.getElementById('sv-'+id);
  function pct(v){return(v-mn)/(mx-mn)*100;}
  function snap(v){v=Math.round(v/st)*st;return Math.max(mn,Math.min(mx,parseFloat(v.toFixed(10))));}
  function apply(v){vl=snap(v);if(fill)fill.style.width=pct(vl)+'%';if(valEl)valEl.textContent=st<1?vl.toFixed(2):Math.round(vl);if(cb)cb(vl);}
  function fromEv(e){var r=tr.getBoundingClientRect();var cx=e.touches?e.touches[0].clientX:e.clientX;return mn+Math.max(0,Math.min(1,(cx-r.left)/r.width))*(mx-mn);}
  apply(vl);
  var dn=false;
  tr.addEventListener('mousedown',function(e){e.preventDefault();e.stopPropagation();dn=true;apply(fromEv(e));});
  document.addEventListener('mousemove',function(e){if(dn)apply(fromEv(e));});
  document.addEventListener('mouseup',function(){dn=false;});
}

initSlider('spd',function(v){STV.spd=v;var p=getP();if(p){p.instance_vars[17]=v;p.instance_vars[37]=v;}if(ST.spdLock)C.setSpd(v);});
initSlider('atk',function(v){STV.atk=v;var p=getP();if(p)p.instance_vars[33]=v;if(ST.atkLock)C.setAtk(v);});
initSlider('hp',function(v){STV.hp=v;C.setHP(v);});

function btn(id,fn){var e=document.getElementById('B-'+id);if(e)e.addEventListener('click',fn);}

btn('killall',function(){C.killAll();msg('Killed all');});
btn('maxall',function(){setTog('godMode',true);setTog('instantKill',true);setTog('spdLock',true);C.setHP(99999);C.setGold(999999);C.setLevel(99);C.setXP(999999);msg('Max Everything ON');});
btn('stopall',function(){C.stopAll();Object.keys(ST).forEach(function(k){setTog(k,false);});document.title='WarCall.io';msg('All stopped');});
btn('fullhp',function(){C.setHP(99999);msg('Full HP');});
btn('fullmana',function(){var p=getP();if(p){p.instance_vars[22]=99999;p.instance_vars[23]=99999;p.instance_vars[74]=99999;}msg('Full Mana');});
btn('gold',function(){C.setGold(parseFloat(document.getElementById('I-gold').value)||0);msg('Gold set');});
btn('level',function(){C.setLevel(parseFloat(document.getElementById('I-level').value)||1);msg('Level set');});
btn('xp',function(){C.setXP(parseFloat(document.getElementById('I-xp').value)||0);msg('XP set');});
btn('kills',function(){C.setKills(parseFloat(document.getElementById('I-kills').value)||0);msg('Kills set');});

var hdr=document.getElementById('__EXH');
var drag=false,ox=0,oy=0,sx=0,sy=0;
hdr.addEventListener('mousedown',function(e){
  drag=true;
  var r=menu.getBoundingClientRect();
  ox=e.clientX-r.left;
  oy=e.clientY-r.top;
  e.preventDefault();
  e.stopPropagation();
});
document.addEventListener('mousemove',function(e){
  if(!drag)return;
  menu.style.left=(e.clientX-ox)+'px';
  menu.style.top=(e.clientY-oy)+'px';
  menu.style.right='auto';
});
document.addEventListener('mouseup',function(){drag=false;});

document.addEventListener('keydown',function(e){
  if(e.key==='Insert'){
    menu.style.display=menu.style.display==='none'?'flex':'none';
  }
});

function sv(id,v){var e=document.getElementById(id);if(e)e.textContent=v;}
function upd(){
  var p=getP();if(!p)return;
  var iv=p.instance_vars;
  var g=getG();var gold=g?g.instance_vars[0]:iv[71];
  var enm=getE().length;
  var hpP=Math.max(0,Math.min(100,iv[20]/iv[18]*100));
  var mpP=Math.max(0,Math.min(100,iv[23]/iv[22]*100));
  var pos=Math.round(p.x)+', '+Math.round(p.y);
  var fps=(rt&&rt.fps)||'-';

  sv('ci-s17',iv[17]);sv('ci-s37',iv[37]);sv('ci-atk',iv[33]);
  sv('ci-enm',enm);sv('ci-kl',iv[35]);sv('ci-dt',iv[36]);
  sv('ci-god',ST.godMode?'ON':'OFF');sv('ci-ik',ST.instantKill?'ON':'OFF');
  sv('ci-frz',ST.freeze?'ON':'OFF');sv('ci-slk',ST.spdLock?'ON':'OFF');sv('ci-alk',ST.atkLock?'ON':'OFF');

  sv('pi-nick',iv[19]);sv('pi-champ',iv[30]);sv('pi-lv',iv[5]);
  sv('pi-hp',Math.round(iv[20])+' / '+iv[18]);
  sv('pi-mana',Math.round(iv[23])+' / '+iv[22]);
  sv('pi-spd',iv[17]);sv('pi-gold',Math.round(gold));
  sv('pi-xp',iv[45]);sv('pi-pos',pos);
  var phb=document.getElementById('pi-hpb');if(phb)phb.style.width=hpP+'%';
  var pmb=document.getElementById('pi-mpb');if(pmb)pmb.style.width=mpP+'%';

  sv('ii-nick',iv[19]);sv('ii-champ',iv[30]);sv('ii-skin',iv[4]);sv('ii-uid',p.uid);
  sv('ii-hp',Math.round(iv[20])+' / '+iv[18]);sv('ii-mana',Math.round(iv[23])+' / '+iv[22]);
  sv('ii-spd',iv[17]);sv('ii-atk',iv[33]);sv('ii-gold',Math.round(gold));
  sv('ii-kl',iv[35]);sv('ii-dt',iv[36]);sv('ii-pos',pos);sv('ii-fps',fps);

  var vb=document.getElementById('ii-vars');
  if(vb){
    var h='';
    iv.forEach(function(v,i){if(v!==0&&v!==''&&v!==null&&v!==undefined)h+='<span style="color:#333">['+i+']</span> <span style="color:#777">'+v+'</span><br>';});
    vb.innerHTML=h;
  }

  if(ST.watchTitle)document.title='HP:'+Math.round(iv[20])+'/'+iv[18]+' SPD:'+iv[17]+' G:'+Math.round(gold)+' Lv:'+iv[5];
}

window.__EXMIT=setInterval(upd,400);
msg('ready');
console.log('EXAMPLE MENU v1.0 | INSERT=hide/show | drag header to move');
})();
